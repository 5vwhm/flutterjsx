{
  "name": "flutterjsx",
  "version": "0.0.3",
  "description": "Jsx in Javascript View For Flutter.dart",
  "main": "src/app.ts",
  "contributors": [
    "Daniel Dezfouli <iamdaniald@gmail.com> (https://github.com/danialdezfouli/)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/danialdezfouli/flutterjsx.git"
  },
  "scripts": {
    "start": "nodemon -p src/app.ts",
    "dev": "ts-node-dev --respawn --clear --watch=lib,src src/app.ts",
    "nodemon": "nodemon src/app.ts"
  },
  "bin": {
    "flutterjsx": "npm run dev"
  },
  "keywords": [
    "flutter",
    "dart",
    "jsx",
    "jsx to dart"
  ],
  "author": "Daniel Dezfouli",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.9.5"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/generator": "^7.9.5",
    "@babel/helper-builder-react-jsx-experimental": "^7.9.5",
    "@babel/helper-plugin-utils": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@babel/traverse": "^7.9.5",
    "@types/node": "^13.11.0",
    "import-jsx": "^4.0.0",
    "jsdoc-parse": "^4.0.1",
    "ts-node": "^8.8.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
